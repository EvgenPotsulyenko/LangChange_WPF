<Window x:Class="Wpf_multiLang.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wpf_multiLang"
        xmlns:control="clr-namespace:Wpf_multiLang.Ui"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <StackPanel>
                <Menu DockPanel.Dock="Top" Background="{x:Null}">
                    <MenuItem x:Name="menuLanguage" Header="{DynamicResource m_menu_Language}" />
                </Menu>
            </StackPanel>
            <control:UserControl_Button Loaded="UserControl_Button_Loaded"></control:UserControl_Button>
        </StackPanel>

    </Grid>
</Window>
